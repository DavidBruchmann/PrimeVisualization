{
    "name": "wdb/prime-visualization",
    "description": "Several Functions for verifying, analyzing and displaying primes.",
    "type": "project",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "DavidBruchmann",
            "email": "david.bruchmann@gmail.com"
        }
    ],
    "minimum-stability": "beta",
    "repositories": [
        {
            "type": "path",
			"url": "./packages/*",
			"options": {
				"symlink": true
			}
		}
    ],
    "autoload": {
        "psr-4": {
            "WDB\\PrimeVisualization\\": "Classes/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WDB\\PrimeVisualization\\": "Tests/"
        }
    },
    "require": {
        "typo3fluid/fluid": "^2.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^8",
        "composer/composer": "*"
    }
}
